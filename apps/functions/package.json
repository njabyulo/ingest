{
  "name": "functions",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "echo 'Functions built by SST'",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:code": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:type": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest run --exclude '**/*integration*.test.ts' --passWithNoTests",
    "test:integration": "vitest run 'src/**/*integration*.test.ts'",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf .turbo",
    "clean:all": "rm -rf .turbo node_modules dist"
  },
  "dependencies": {
    "@ingest/shared": "workspace:*",
    "@aws-sdk/client-s3": "catalog:",
    "@aws-sdk/s3-request-presigner": "catalog:",
    "hono": "catalog:"
  },
  "devDependencies": {
    "@types/aws-lambda": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
