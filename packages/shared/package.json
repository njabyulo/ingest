{
  "name": "@ingest/shared",
  "version": "1.0.0",
  "private": true,
  "description": "Shared types, utilities, schemas, and constants",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.mjs",
      "require": "./dist/constants/index.js",
      "default": "./dist/constants/index.mjs"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.mjs",
      "require": "./dist/schemas/index.js",
      "default": "./dist/schemas/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "default": "./dist/types/index.mjs"
    },
    "./types/ingest": {
      "types": "./dist/types/ingest/index.d.ts",
      "import": "./dist/types/ingest/index.mjs",
      "require": "./dist/types/ingest/index.js",
      "default": "./dist/types/ingest/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "default": "./dist/utils/index.mjs"
    },
    "./utils/ingest": {
      "types": "./dist/utils/ingest/index.d.ts",
      "import": "./dist/utils/ingest/index.mjs",
      "require": "./dist/utils/ingest/index.js",
      "default": "./dist/utils/ingest/index.mjs"
    },
    "./constants/ingest": {
      "types": "./dist/constants/ingest/index.d.ts",
      "import": "./dist/constants/ingest/index.mjs",
      "require": "./dist/constants/ingest/index.js",
      "default": "./dist/constants/ingest/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "pnpm lint:type && pnpm lint:code",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo",
    "clean:all": "rm -rf .turbo node_modules dist"
  },
  "dependencies": {},
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}

